<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.collections.FXCollections?>
<?import java.lang.String?>
<ToolBar xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml" prefWidth="220" HBox.hgrow="ALWAYS">
    <TextField focusTraversable="false" promptText="View Name"/>
    <Separator/>
    <Label text="Type:"/>
    <ChoiceBox value="Entry">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Entry"/>
                <String fx:value="Team"/>
                <String fx:value="Match"/>
                <String fx:value="Scout"/>
                <String fx:value="Event"/>
            </FXCollections>
        </items>
    </ChoiceBox>
    <Separator/>
    <Button text="Filter Rows"/>
    <Separator/>
    <CheckBox text="Transpose"/>
    <Separator/>
    <Button text="Format Rules"/>
    <Separator/>
    <Button text="Create Plot"/>
    <Separator/>
    <Label text="Pivot:"/>
    <ChoiceBox value="None" prefWidth="150">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="None"/>
                <String fx:value="Team"/>
                <String fx:value="Match"/>
                <String fx:value="Alliance"/>
                <String fx:value="Auto Line"/>
                <String fx:value="Scale"/>
                <String fx:value="Switch"/>
                <String fx:value="Climb"/>
            </FXCollections>
        </items>
    </ChoiceBox>
    <Separator/>
    <Label text="Find:"/>
    <TextField focusTraversable="false" prefWidth="240"/>
    <Separator/>
    <Button text="Export"/>
    <Button text="Duplicate"/>
</ToolBar>