<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<?import java.lang.*?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml" prefWidth="220">
    <Label text="Columns" prefWidth="220" alignment="CENTER">
        <style>
            -fx-text-fill: #3c5c94;
            -fx-font-weight: bold;
            -fx-padding: 5 0 5 0;
        </style>
    </Label>
    <TextField focusTraversable="false" prefWidth="100" promptText="Search Metrics" alignment="CENTER"/>
    <ListView VBox.vgrow="ALWAYS">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <CheckBox text="Team"/>
                <CheckBox text="Match"/>
                <CheckBox text="Alliance"/>
                <CheckBox text="Auto Line"/>
                <CheckBox text="Scale"/>
                <CheckBox text="Switch"/>
                <CheckBox text="Climb"/>
            </FXCollections>
        </items>
    </ListView>
</VBox>

