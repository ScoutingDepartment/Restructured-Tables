<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" prefWidth="400" prefHeight="600" spacing="10">
    <style>
        -fx-padding: 10 10 10 10;
    </style>
    <VBox VBox.vgrow="ALWAYS" >
        <HBox spacing="10">
            <Label text="App User:"/>
            <Label text="Yu Liu" style="-fx-font-weight:bold; -fx-text-fill: #000000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Event ID:"/>
            <Label text="2018iri" style="-fx-font-weight:bold; -fx-text-fill: #000000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Event Name:"/>
            <Label text="Indiana Robotics Invitational 2018" style="-fx-font-weight:bold; -fx-text-fill: #000000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Scouted Matches:"/>
            <Label text="36/105" style="-fx-font-weight:bold; -fx-text-fill: #3c5c94"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Total Teams:"/>
            <Label text="70" style="-fx-font-weight:bold; -fx-text-fill: #3c5c94"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Wrong Data:"/>
            <Label text="100/216" style="-fx-font-weight:bold; -fx-text-fill: #ff0000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Verified Data:"/>
            <Label text="0/216" style="-fx-font-weight:bold; -fx-text-fill: #008000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Python Status:"/>
            <Label text="Using Python 3.7.1 Embedded" style="-fx-font-weight:bold; -fx-text-fill: #000000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Analysis Status:"/>
            <Label text="Exited with no errors" style="-fx-font-weight:bold; -fx-text-fill: #008000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Last Analyzed:"/>
            <Label text="20 minutes ago" style="-fx-font-weight:bold; -fx-text-fill: #f08000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Total Saved Views:"/>
            <Label text="12" style="-fx-font-weight:bold; -fx-text-fill: #3c5c94"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Using Memory:"/>
            <Label text="30M" style="-fx-font-weight:bold; -fx-text-fill: #3c5c94"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Backup Status:"/>
            <Label text="100% Done" style="-fx-font-weight:bold; -fx-text-fill: #008000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Backup Path:"/>
            <Label text="{@AppData}/TablesBackup" style="-fx-font-weight:bold; -fx-text-fill: #000000"/>
        </HBox>
    </VBox>
    <HBox alignment="CENTER_RIGHT" spacing="10">
        <Button text="Set TBA Key"/>
        <Button text="Set User"/>
        <Button text="Show Logs"/>
        <Button text="Close"/>
    </HBox>
</VBox>