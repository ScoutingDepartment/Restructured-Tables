<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" prefWidth="400" prefHeight="225">
    <style>
        -fx-padding: 10 10 10 10;
    </style>
    <VBox VBox.vgrow="ALWAYS">
        <HBox spacing="10">
            <Label text="Event:"/>
            <Label text="Indiana Robotics Invitational 2018" style="-fx-font-weight:bold; -fx-text-fill: #000000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Scouted Matches:"/>
            <Label text="36/105" style="-fx-font-weight:bold; -fx-text-fill: #3c5c94"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Total Teams:"/>
            <Label text="70" style="-fx-font-weight:bold; -fx-text-fill: #3c5c94"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Wrong Data:"/>
            <Label text="100/216" style="-fx-font-weight:bold; -fx-text-fill: #ff0000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Verified Data:"/>
            <Label text="0/216" style="-fx-font-weight:bold; -fx-text-fill: #008000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Backup Status:"/>
            <Label text="100% Done" style="-fx-font-weight:bold; -fx-text-fill: #000000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Python Status:"/>
            <Label text="Using Python 3.7.1" style="-fx-font-weight:bold; -fx-text-fill: #000000"/>
        </HBox>
        <HBox spacing="10">
            <Label text="Analysis Status:"/>
            <Label text="Exited with no errors" style="-fx-font-weight:bold; -fx-text-fill: #008000"/>
        </HBox>
    </VBox>
    <HBox alignment="CENTER_RIGHT" spacing="10">
        <Button text="Show Logs"/>
        <Button text="Ok"/>
    </HBox>
</VBox>